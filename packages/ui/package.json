{
    "name": "@morsz/ui",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "sideEffects": [
        "**/*.css"
    ],
    "files": [
        "dist"
    ],
    "exports": {
        "./*": "./src/components/*.tsx",
        "./utils": "./src/lib/utils.ts",
        "./hooks/*": "./src/hooks/*.ts",
        "./styles": "./dist/index.css",
        "./styles/globals": "./src/styles/globals.css",
        "./styles/fonts": "./src/styles/fonts.ts",
        "./styles/unicode-fonts": "./src/styles/unicode-fonts.css"
    },
    "scripts": {
        "build:styles": "bunx tailwindcss -i ./src/styles/globals.css -o ./dist/index.css",
        "dev:styles": "bunx tailwindcss -i ./src/styles/globals.css -o ./dist/index.css --watch",
        "lint": "eslint src --max-warnings 0",
        "format": "prettier --cache --write ."
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-toggle": "^1.1.10",
        "@radix-ui/react-toggle-group": "^1.1.11",
        "@radix-ui/react-tooltip": "^1.2.8",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "lucide-react": "^0.475.0",
        "motion": "^12.23.12",
        "next-themes": "^0.4.6",
        "react": "workspace:*",
        "react-day-picker": "^9.9.0",
        "react-dom": "workspace:*",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "tailwindcss": "^4.1.12"
    }
}
